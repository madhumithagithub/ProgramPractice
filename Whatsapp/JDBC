package Whatsapp1;
import java.sql.*;
public class Jdbc {
	
	public static Connection jdbcConnection(){
	        try{
	           return DriverManager.getConnection("jdbc:mysql://localhost:3306/whatsapp1","root","Madhu*10");
	            
	        }catch (Exception e){
	            e.printStackTrace();
	        }
	        return null;
	    }

	}

Eg 1:Input:
        String 1: test123string
         String 2: 123
        Output: 4
Eg 2: Input:
        String 1: testing12
        String 2: 1234 
        Output: -1
        
        package oops;
import java.util.*;
public class Main2
{
	public static void main(String[] args)
	{
		Scanner s =new Scanner(System.in);
		String s1 = "test123string";
		String s2 ="1234";
		System.out.println(s1.indexOf(s2));
	}
}

  

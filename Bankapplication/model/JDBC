package Bankapplication1;
import java.sql.*;
import java.sql.*;

public class JDBC {
	public static Connection jdbcConnection() {
		try {
			return DriverManager.getConnection("jdbc:mysql://localhost:3306/bankapplication", "root", "Madhu*10");

		} catch (Exception e) {
			e.printStackTrace();
		}
		return null;
	}
}
